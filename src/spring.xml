<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
	<bean id="circle" class="com.main.functions.Circle"> 
		<property name="pointA" ref="point0">
		</property>
	</bean>
	
	<bean id="point0" class="com.main.functions.Point"> 
		<property name="x" value="5">
		</property>
		<property name="y" value="5">
		</property>
	</bean>
	<bean id="point1" class="com.main.functions.Point"> 
		<property name="x" value="4">
		</property>
		<property name="y" value="4">
		</property>
	</bean>
	<bean id="point2" class="com.main.functions.Point"> 
		<property name="x" value="3">
		</property>
		<property name="y" value="3">
		</property>
	</bean>

</beans>